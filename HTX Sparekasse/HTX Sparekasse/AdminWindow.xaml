<Window x:Class="HTX_Sparekasse.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HTX_Sparekasse"
        mc:Ignorable="d"
        Title="HTX Sparekasse" Height="330" Width="400">
    <Grid>
        <TabControl x:Name="tabControl" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <TabItem Header="Betjeningspanel" Margin="-2,0">
                <Grid Background="#FFE5E5E5" Margin="-2"/>
            </TabItem>
            <TabItem Header="Brugere" Margin="1,0,-5,0">
                <Grid Background="#FFE5E5E5" Margin="-2">
                    <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="271" VerticalAlignment="Top" Width="372"/>
                </Grid>
            </TabItem>
            <TabItem Header="Overførsler" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Width="72" Margin="4,0,-4,0">
                <Grid Background="#FFE5E5E5" Margin="-2"/>
            </TabItem>
            <TabItem Header="Indstillinger" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Width="74" Margin="3,0,-3,0">
                <Grid Background="#FFE5E5E5" Margin="-2"/>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
